                  <label> 
                  <input type="checkbox" id="customCheck1">                                     
                    checkbox unchecked
                  </label>
                </div> 
                <div class="checkbox">  
                  <label>                
                  <input type="checkbox" id="customCheck2" checked="checked">                                    
                    checkbox checked
                  </label>
                </div>   
                <div class="checkbox">   
                  <label>                 
                  <input type="checkbox" id="customCheck3" checked="checked" disabled>
